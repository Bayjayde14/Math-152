        "import numpy as np\n",
        "def matrix_exp(M):\n",
        "  result = np.eye(M.shape[0])\n",
        "  for i in range(1, 21):\n",
        "    result += matrix_pow(M, i) / np.math.factorial(i)\n",
        "  return result"
