      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "def matrix_pow(M, e):\n",
        "  if e == 0:\n",
        "    return np.eye(M.shape[0])\n",
        "  else:\n",
        "    result = M\n",
        "    for i in range(e-1):\n",
        "      result = np.dot(result, M)\n",
        "    return result \n",
        "    "
      ],
      "metadata": {
        "id": "sGOaoEPkYAC4"
      },
      "execution_count": 98,
      "outputs": []
    },
